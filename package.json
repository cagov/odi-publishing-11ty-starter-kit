{
  "name": "odi-publishing-11ty-sandbox",
  "version": "0.0.1",
  "description": "A working copy of odi-publishing-11ty-starter-kit for demos, training and testing.",
  "main": "index.html",
  "scripts": {
    "build": "eleventy",
    "build:dev": "cross-env NODE_ENV=development eleventy",
    "watch": "eleventy --serve",
    "watch:dev": "cross-env NODE_ENV=development eleventy --serve --incremental",
    "start": "npm run watch",
    "test": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "test:cypress:tool": "npx cypress open",
    "test:cypress": "npm-run-all test:server:background test:cypress:tool",
    "test:playwright": "npm-run-all test:server:background test",
    "test:playwright:headed": "npm-run-all test:server:background test:headed",
    "test:server": "npx web-dev-server --root-dir docs/",
    "test:server:background": "npx web-dev-server --root-dir docs/ &",
    "test:setup": "npx playwright install-deps chromium && npx playwright install",
    "git:merge:dev-into-main": "./src/sh/git-merge-development-into-main.sh",
    "git:merge:main-into-dev": "./src/sh/git-merge-main-into-development.sh",
    "git:merge:staging-into-main": "./src/sh/git-merge-staging-into-main.sh",
    "git:merge:main-into-staging": "./src/sh/git-merge-main-into-staging.sh"
  },
  "keywords": [
    "ca.gov",
    "@cagov",
    "odi-publishing"
  ],
  "author": "Office of Digital Innovation",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0-beta.3",
    "@cagov/11ty-build-system": "^0.1.3",
    "@csstools/postcss-sass": "^4.0.0",
    "@playwright/test": "^1.14.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@web/dev-server": "^0.1.22",
    "autoprefixer": "^10.2.5",
    "aws-sdk": "^2.989.0",
    "axe-core": "^4.3.3",
    "axe-playwright": "^1.1.8",
    "cheerio": "^1.0.0-rc.10",
    "chokidar-cli": "^2.1.0",
    "clean-css": "^5.1.2",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.11",
    "cypress": "^8.3.1",
    "cypress-axe": "^0.13.0",
    "git-branch": "^2.0.1",
    "html-minifier": "^4.0.0",
    "mochawesome": "^6.3.1",
    "node-fetch": "^2.6.1",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "playwright-mocha": "^2.0.2",
    "postcss-scss": "^4.0.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "@cagov/ds-accordion": "^1.0.6",
    "@cagov/ds-agency-footer": "^1.0.1",
    "@cagov/ds-back-to-top": "^1.0.1",
    "@cagov/ds-base-css": "^1.0.8",
    "@cagov/ds-branding": "^1.0.5",
    "@cagov/ds-card-grid": "^1.0.2",
    "@cagov/ds-content-navigation": "^1.0.5",
    "@cagov/ds-dropdown-menu": "^1.0.13",
    "@cagov/ds-feature-card": "^1.0.2",
    "@cagov/ds-feedback": "^1.0.9",
    "@cagov/ds-google-translate": "^1.0.0",
    "@cagov/ds-minus": "^1.0.0",
    "@cagov/ds-pagination": "^1.0.4",
    "@cagov/ds-pdf-icon": "^1.0.0",
    "@cagov/ds-plus": "^1.0.0",
    "@cagov/ds-regulatory-outline": "^1.0.0",
    "@cagov/ds-skip-to-content": "^1.0.0",
    "@cagov/ds-statewide-footer": "^1.0.4",
    "@cagov/ds-statewide-header": "^1.0.10",
    "@cagov/ds-step-list": "^1.0.1"
  }
}
